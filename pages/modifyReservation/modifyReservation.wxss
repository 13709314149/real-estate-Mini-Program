/**index.wxss**/
.container {
  background: #f2f2f2;
  padding-bottom: 40rpx;
}
.row{
  background: white;
  border-bottom: 1rpx solid #f2f2f2;
  display: flex;
  height: 110rpx;
  position: relative;
  font-size:32rpx;
  padding: 0 30rpx;
  align-items: center;
}
.row>label{
  flex-shrink: 0;
  width: 250rpx;
}
.row>input{
  flex: 1 1 auto;
}

.row>picker{
  flex: 1 1 auto;
  color: #9a9a9a;
}
.placeholderClass{
  color: #9a9a9a;
  font-weight: bold;
}
.row:nth-of-type(3){
  margin-bottom: 150rpx;
}
.btn-default{
  margin: 0 30rpx;
}